{
  "expo": {
    "name": "Juked",
    "slug": "Juked",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.1.7",
    "orientation": "portrait",
    "icon": "./assets/icon-ios-1024@1x.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.juked.jukedapp"
    }
  }
}
